import random
def better_than_average(your_points):
    class_points = []
    for i in range(1, 30):
        class_points.append(random.randint(1, 12))
    average_points = sum(class_points)/len(class_points)
    print("average_points:", average_points)
    print("your_points:", your_points)
    if average_points < your_points:
        print("Your points better!")
    else:
        print("Your points worse!")


better_than_average(7)

better_than_average(10)
